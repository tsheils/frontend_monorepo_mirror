      ],
      "@ncats-frontend-library/idg-tdl-indicator": [
        "libs/idg-tdl-indicator/src/index.ts"
      ],
      "@ncats-frontend-library/dynamic-app": ["libs/dynamic-app/src/index.ts"],
      "@ncats-frontend-library/ncats-form": ["libs/ncats-form/src/index.ts"],
      "@ncats-frontend-library/sidenavs/navigation-sidepanel": [
        "libs/sidenavs/navigation-sidepanel/src/index.ts"
      ],
      "@ncats-frontend-library/sidenavs/curation-sidepanel": [
        "libs/sidenavs/curation-sidepanel/src/index.ts"
      ],
      "@ncats-frontend-library/sidenavs/facet-sidepanel": [
