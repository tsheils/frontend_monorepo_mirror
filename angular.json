{
  "version": 1,
  "projects": {
    "ncats-frontend-demo": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ncats-frontend-demo",
      "sourceRoot": "apps/ncats-frontend-demo/src",
      "prefix": "ncats-frontend-library",
      "architect": {
        "build": {
          "builder": "ngx-library-assets:browser",
          "options": {
            "outputPath": "dist/apps/ncats-frontend-demo",
            "index": "apps/ncats-frontend-demo/src/index.html",
            "main": "apps/ncats-frontend-demo/src/main.ts",
            "polyfills": "apps/ncats-frontend-demo/src/polyfills.ts",
            "tsConfig": "apps/ncats-frontend-demo/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/ncats-frontend-demo/src/favicon.ico",
              "apps/ncats-frontend-demo/src/assets"
            ],
            "styles": ["apps/ncats-frontend-demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ncats-frontend-demo/src/environments/environment.ts",
                  "with": "apps/ncats-frontend-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-library-assets:dev-server",
          "options": {
            "browserTarget": "ncats-frontend-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ncats-frontend-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ncats-frontend-demo:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ncats-frontend-demo/tsconfig.app.json",
              "apps/ncats-frontend-demo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ncats-frontend-demo/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ncats-frontend-demo/jest.config.js",
            "tsConfig": "apps/ncats-frontend-demo/tsconfig.spec.json",
            "setupFile": "apps/ncats-frontend-demo/src/test-setup.ts"
          }
        }
      }
    },
