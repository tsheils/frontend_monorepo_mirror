    "gard-data-hub": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/gard-data-hub",
      "sourceRoot": "apps/gard-data-hub/src",
      "prefix": "ncats-frontend-library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/gard-data-hub",
            "index": "apps/gard-data-hub/src/index.html",
            "main": "apps/gard-data-hub/src/main.ts",
            "polyfills": "apps/gard-data-hub/src/polyfills.ts",
            "tsConfig": "apps/gard-data-hub/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/gard-data-hub/src/favicon.ico",
              "apps/gard-data-hub/src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/styles/src/styles/"]
            },
            "styles": ["apps/gard-data-hub/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/gard-data-hub/src/environments/environment.ts",
                  "with": "apps/gard-data-hub/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gard-data-hub:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gard-data-hub:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gard-data-hub:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/gard-data-hub/tsconfig.app.json",
              "apps/gard-data-hub/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/gard-data-hub/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/gard-data-hub/jest.config.js",
            "tsConfig": "apps/gard-data-hub/tsconfig.spec.json",
            "setupFile": "apps/gard-data-hub/src/test-setup.ts"
          }
        }
      }
    },
